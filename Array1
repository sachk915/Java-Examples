public class Array1 
{
	public static void main(String[] ss) 
	{
		int size=Integer.parseInt(args[0]);
		int []a=new int[size];

		for(int i=0;i<a.length;i++)
			a[i]=Integer.parseInt(args[i+1]);

		System.out.println("Array Elements are");

		for(int i=0;i<a.length;i++)
			System.out.println(a[i]);
	}
}
